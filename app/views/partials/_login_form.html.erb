<%= form_for :session, url: session_path, :html=>{:id => "loginform", :name => "form"} do |form| %>
	
	  <div>
	    <%= form.email_field :email, class: 'login_field', :placeholder => "Email"%>
	  </div>
	  <div>
	    <%= form.password_field :password, class: 'login_field', :placeholder => "Password"%>
	  </div>
	  <div>
	    <%= form.submit "Sign in", class: 'login_submit button' %>
	  </div>
	  <ul class="login_errors">
    	<li></li>
  	</ul>
<% end %>